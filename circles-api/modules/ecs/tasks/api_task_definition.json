[
  {
    "name": "circles-api-ecr",
    "image": "${image}",
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8080
      }
    ],
    "memory": 300,
    "networkMode": "awsvpc",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group_name}",
        "awslogs-region": "${log_group_region}",
        "awslogs-stream-prefix": "circles_api"
      }
    },
    "environment": [
      {
        "name": "COGNITO_POOL_ID",
        "value": "${cognito_pool_id}"
      },
      {
        "name": "COGNITO_POOL_REGION",
        "value": "${region}"
      },
      {
        "name": "PGUSER",
        "value": "${database_user}"
      },
      {
        "name": "PGHOST",
        "value": "${database_host}"
      },
      {
        "name": "PGPASSWORD",
        "value": "${database_password}"
      },
      {
        "name": "PGDATABASE",
        "value": "${database_name}"
      },
      {
        "name": "PGPORT",
        "value": "${database_port}"
      },
      {
        "name": "ANDROID_GCM_PLATFORM_ARN",
        "value": "${android_platform_gcm_arn}"
      },
      {
        "name": "PRIVATE_KEY",
        "value": "${private_key}"
      },
      {
        "name": "NETWORK_ID",
        "value": "10999"
      },
      {
        "name" : "COGNITO_POOL_JWT_KID",
        "value": "${cognito_pool_jwt_kid}"
      },
      {
        "name" : "COGNITO_POOL_JWT_N",
        "value": "${cognito_pool_jwt_n}"
      }
    ]
  }
]